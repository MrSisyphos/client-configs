{
  "name": "client-configs",
  "packageManager": "yarn@3.6.1",
  "scripts": {
    "setup": "eth-sdk",
    "test": "concurrently --raw --kill-others --success last \"anvil --silent\" \"jest --runInBand\"",
    "test:watch": "concurrently --raw --kill-others --handle-input --passthrough-arguments \"anvil --silent\" \"jest --runInBand --watch {@}\" --",
    "check:prettier": "prettier '**/*.{js,ts,tsx,json,md}' --list-different",
    "check:types": "tsc --noEmit",
    "fix:prettier": "prettier '**/*.{js,ts,tsx,json,md}' --write"
  },
  "dependencies": {
    "@dethcrypto/eth-sdk": "^0.3.4",
    "@dethcrypto/eth-sdk-client": "^0.1.6",
    "@ethersproject/abi": "^5.7.0",
    "@ethersproject/bignumber": "^5.7.0",
    "@ethersproject/providers": "^5.7.2",
    "@typechain/ethers-v5": "^11.0.0",
    "@types/jest": "^29.5.3",
    "@types/wait-on": "^5.3.1",
    "concurrently": "^8.2.2",
    "defi-kit": "^0.1.20",
    "ethers": "^5.7.2",
    "jest": "^29.6.1",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typechain": "^8.2.0",
    "typescript": "^5.1.6",
    "wait-on": "^7.0.1",
    "yargs": "^17.7.2",
    "zodiac-roles-sdk": "^2.2.4"
  }
}
